<div class="center">

    <h1 class="subtitle RobotoMono">Look empty here. Let's post something!</h1>
    <button class="plusBtn addPost">+</button>

    {{#each Post.postCount}}
    <div class="postCenter">
        <div class="post">
            <h2 class=" RobotoMono postTitle">{{postTitle}}</h2>
            <h2 class="RobotoMono postUsername">{{postName}}</h2>
            <p class=" RobotoMono postBody"></p>
            <div class="RobotoMono postComment">
            {{#each Post.commentCount}}
                {{commentBody}}
                {{/each}}
            </div>
            {{/each}}
            <button class="RobotoMono postBtn">Comment</button>
        </div>
    </div>
    {{/each}}

</div>